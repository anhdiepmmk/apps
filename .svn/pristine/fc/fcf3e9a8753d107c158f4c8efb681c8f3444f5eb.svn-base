.navbar{
    background:#000;
}
.navbar-nav>li.active>a, .navbar-nav>li.active>a:hover, .navbar-nav>li.active>a:focus{
    background: #62b23f;
}
.navbar-nav>li.active>a:before, .navbar-nav>li>a:hover:before, .navbar-nav>li>a:focus:before, .extra-nav-class a.dropdown-toggle:before{
    border-bottom: 6px solid #62b23f;
}
.navbar-nav>li>a:hover, .navbar-nav>li>a:focus{
    background-color: #62b23f;
}
.navbar-fixed-top{
    top:0px;
}
@media (min-width: 992px){
    .content {
        padding-top: 110px;
    }
}
.nborder .breadcrumbs h1,
.breadcrumbs h4,
.breadcrumbs h5{
    border-left: 5px solid #62b23f;
    padding-left: 10px; 
}
.breadcrumbs{
    padding-left: 0px;
}
.btn-primary{
    background-color: #62b23f;
    border-color: #59A03A;
}
.btn-primary.active, .btn-primary.focus, .btn-primary:active, .btn-primary:focus, .btn-primary:hover, .open>.dropdown-toggle.btn-primary{
    background-color: #62b23f;
    border-color: #59A03A;
}
.navbar-nav>li>a{
    color:#737373
}
.copyright{
    background:#000;
}
.copyright ul.footermenu li a{
    color:#999;
}
.wowmenu.tiny{
    background: rgba(0, 0, 0, 0.84) !important;
}
.tab-content li{
    clear: none;
}
.tab-content{
    padding-top: 60px;
}
.navbar>.container .navbar-brand{
    margin-top:-10px;
}
.modal-md .modal-content{
    width:50%px;
    margin:auto;
}
.no_shadow .modal-content{
    box-shadow: none;
}
.no_shadow .modal-dialog{
    margin-top: 0px;
}
#signin .form-group{
    margin-bottom: 10px;
}
.mt-10{
    margin-top: 10px;
}
.my_profile,.change_password{
    margin-top: 20px;
}
.my_profile .form-group{
    margin-bottom: 5px;
}
.menu_profile ul{
    margin-top: 10px;
}
.menu_profile ul li{
    list-style:none;
    padding:5px 0px;
}
.menu_profile ul li a{
    padding-left: 10px;
    display: block;
    padding: 10px;
    color: white;
    font-size: 14px;
    background:#B7B7B7;
    text-decoration:none;

    font-weight: 500;
    border-left: 2px solid rgba(98, 178, 63, 0.55);


    -webkit-transition: all 0.2s ease-in-out;
    -moz-transition: all 0.2s ease-in-out;
    -o-transition: all 0.2s ease-in-out;
    -ms-transition: all 0.2s ease-in-out;
    transition: all 0.2s ease-in-out;
}
.menu_profile ul li a:hover{
    background: #62b23f;
    font-weight:500;
}
.menu_profile ul li a.active{
    background: #62b23f;
    font-weight:500;
}
.table-referral{
    margin-bottom:0px;
}
.my-referral .form-group{
    margin-top: 0px;
}
.my-referral .form-control{
    margin-bottom: 0px;
}
.navbar-nav>li>.dropdown-menu:before {
    border-bottom: 6px solid #333333;
    border-left: 6px solid transparent;
    border-right: 6px solid transparent;
    left: 30px;
    content: "";
    height: 0;
    margin-right: -3px;
    pointer-events: none;
    position: absolute;
    width: 0;
    bottom: 100%;
}
.dropdown-menu .divider{
    background-color: #26262D;
    margin:0px;
}
.dropdown-menu span{
    font-size: 14px;
    display: block;
    padding-left: 20px;
    padding-top: 10px;
    padding-bottom: 10px;
    color: #62B23F;
    font-weight: bold;
    background: #333;
}


@media (max-width: 1200px){
    #search_large{
        width:30%;
    }
}
@media screen and (max-width: 992px) and (min-width: 600px){
    .tinynav{
        background: #000;
    }
}
@media (max-width: 980px){
    #search_large{
        display: none;
    }
    /*    .content{
            padding-top: 0px;
        }*/
    .navbar{
        background: url(../img/head.png) #fff repeat;
        height: 115px;
    }
    .tinynav{
        background:#000;
    }
    .tinynav::selection{
        background: #62b23f;
    }
}
@media screen and (max-width: 600px){
    .container>.navbar-header, .container>.navbar-collapse {
        margin-top: 40px;
    }

}